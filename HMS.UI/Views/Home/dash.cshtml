                    @foreach (var reservationinfo in Model.reservation)
                    {
                        @if (reservationinfo.Date == DateTime.Today.AddDays(4).Date)
                        {
                            <div class="d-flex align-items-center mb-6">
                                <!--begin::Bullet-->
                                @if (x == 0 || x == 4 || x == 8)
                                {
                                    <span data-kt-element="bullet" class="bullet bullet-vertical d-flex align-items-center min-h-70px mh-100 me-4 bg-success"></span>
                                }
                                @if (x == 1 || x == 5 || x == 9)
                                {
                                    <span data-kt-element="bullet" class="bullet bullet-vertical d-flex align-items-center min-h-70px mh-100 me-4 bg-danger"></span>
                                }
                                @if (x == 2 || x == 6 || x == 10)
                                {
                                    <span data-kt-element="bullet" class="bullet bullet-vertical d-flex align-items-center min-h-70px mh-100 me-4 bg-warning"></span>
                                }
                                @if (x == 3 || x == 7 || x == 11)
                                {
                                    <span data-kt-element="bullet" class="bullet bullet-vertical d-flex align-items-center min-h-70px mh-100 me-4 bg-info"></span>
                                }
                                <!--end::Bullet-->
                                <!--begin::Info-->
                                <div class="flex-grow-1 me-5">
                                    <!--begin::Time-->
                                    <div class="text-gray-800 fw-semibold fs-2">
                                        @reservationinfo.Time_Start.ToShortTimeString()
                                        -
                                        @reservationinfo.Time_End.ToShortTimeString()
                                    </div>
                                    <!--end::Time-->
                                    <!--begin::Description-->
                                    <div class="text-gray-700 fw-semibold fs-6">
                                        @reservationinfo.Name
                                    </div>
                                    <!--end::Description-->
                                    <!--begin::Link-->
                                    <div class="text-gray-400 fw-semibold fs-7">
                                        محجوزة بواسطة
                                        <!--begin::Name-->
                                        <a href="#" class="text-primary opacity-75-hover fw-semibold">@reservationinfo.User_Name</a>
                                        <!--end::Name-->
                                    </div>
                                    <!--end::Link-->
                                </div>
                                <!--end::Info-->
                                <!--begin::Action-->
                                <div class="btn btn-sm btn-light">اسم القاعة</div>
                                <div class="btn btn-sm btn-light">@reservationinfo.Hall_name</div>
                                <!--end::Action-->
                            </div>
                            x++;
                        }
                    }

                    <!--end::Wrapper-->
                </div>
                <!--end::Tap pane-->
                <!--begin::Tap pane-->
                <!--end::Tap pane-->
                <!--begin::Tap pane-->

            </div>
